{
  "openapi": "3.0.0",
  "paths": {
    "/users": {
      "get": {
        "summary": "Get all users",
        "tags": [
          "Users"
        ],
        "description": "Returns a list of all registered users.",
        "responses": {
          "200": {
            "description": "List of users retrieved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/User"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "b12345"
          },
          "name": {
            "type": "string",
            "example": "Tarun Nayaka"
          },
          "email": {
            "type": "string",
            "example": "tarun@example.com"
          },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "example": "2025-01-01T10:20:30Z"
          }
        }
      }
    }
  }
}